{
  "version": 2,
  "name": "employee-management-system",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/employee-management-frontend",
  "framework": "angular",
  "installCommand": "npm install",
  "build": {
    "env": {
      "NODE_VERSION": "18"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}